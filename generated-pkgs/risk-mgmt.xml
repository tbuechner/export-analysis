<package xmlVersion="1.0" internalName="cf.cplace.riskManagment" version="1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../package-thinned-out-schema.xsd">
